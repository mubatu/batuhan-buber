---
import Layout from '../layouts/Layout.astro';
import '../styles/global.css';
---

<Layout title="Resume">
  <main class="coming-soon">
    <h1>Coming soon</h1>
  </main>
</Layout>

<style>
  .coming-soon {
    display: flex;
    align-items: center;
    justify-content: center;
    min-height: calc(100vh - 80px);
    text-align: center;
  }

  h1 {
    font-size: 2.5rem;
    font-weight: 300;
    color: var(--color-text-primary);
  }
</style>

